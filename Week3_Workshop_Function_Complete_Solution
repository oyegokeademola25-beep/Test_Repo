            "larger circle area: 200.96\n",
            "combined_area: 279.46000000000004\n"
          ]
        }
      ],
      "source": [
        "# Solution:\n",
        "def circle_area(radius):\n",
        "  return 3.14 * radius * radius\n",
        "area_1 = circle_area(5)\n",
        "area_2 = circle_area(8)\n",
        "combined_area = area_1 + area_2\n",
        "print(f'circle area: {area_1}')\n",
        "print (f'larger circle area: {area_2}')\n",
        "print(f'combined_area: {combined_area}')"
      ]
